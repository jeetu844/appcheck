{
  "name": "AppCheck",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "npx react-native start --experimental-debugger --reset-cache",
    "test": "jest",
    "clean": "cd android && ./gradlew clean",
    "release": "cd android && ./gradlew aR",
    "bundle": "cd android && ./gradlew bR",
    "x": "cd ios && xed .",
    "pod": "cd ios && pod install",
    "releaseInstall": "cd android && ./gradlew aR && adb install app/build/outputs/apk/release/app-release.apk",
    "removePod": "rm -rf ~/Library/Developer/Xcode/DerivedData/ && rm -rf build && rm -rf Podfile.lock && rm -rf Pods",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "i18next": "^23.14.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-native": "0.75.4",
    "react-native-blob-util": "^0.19.11",
    "react-native-compressor": "^1.8.25",
    "react-native-device-info": "^11.1.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.19.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.0",
    "react-native-toast-message": "^2.2.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
