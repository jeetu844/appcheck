platform :ios do
    desc "Build the iOS app for physical devices"
    lane :build do
      update_code_signing_settings( 
        use_automatic_signing: true,
        team_id: "HR7RS84V6F"  # Replace with your team ID
      )
  
      build_ios_app(
        workspace: "AppCheck.xcworkspace",
        scheme: "AppCheck",
        export_method: "development",
        output_directory: "./build",
        codesigning_identity: "iPhone Developer",
        export_team_id: "HR7RS84V6F"  # Ensure this matches your team ID
      )
    end
  end
  